<xs:simpleType name="db2_timestamp_type">
  <xs:annotation>
    <xs:documentation>
    Timestamp in the following format: YYYY-MM-DD-HH.MM.SS.FFFFFF
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:string">
    <xs:maxLength value = "26" />
    <xs:pattern value="[0-9]{4}-[0-9]{2}-[0-9]{2}-[0-9]{2}.[0-9]{2}.[0-9]{2}.[0-9]{6}"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name = "db_object_name_type" >
  <xs:annotation>
