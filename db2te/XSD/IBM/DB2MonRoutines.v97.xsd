<xs:simpleType name = "threshold_yes_no_type" >
  <xs:annotation>
     <xs:documentation>
       Indicates whether a threshold was violated or queued.
       Valid values are 1 (YES) or 0 (NO).
     </xs:documentation>
  </xs:annotation>
   <xs:restriction base = "xs:short" >
      <xs:maxInclusive value = "1" />
   </xs:restriction>
</xs:simpleType>

<xs:simpleType name = "threshold_value_type" >
  <xs:annotation>
     <xs:documentation>
       Threshold values are simple strings, as they may be
       numbers, or other types such as timestamps for some thresholds.
     </xs:documentation>
  </xs:annotation>
   <xs:restriction base = "xs:long" />
</xs:simpleType>



