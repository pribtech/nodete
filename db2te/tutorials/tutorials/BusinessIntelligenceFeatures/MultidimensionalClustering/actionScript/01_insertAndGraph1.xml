<actionPanel>    <flow>free</flow>    <useConsole>yes</useConsole>    <overview>    </overview>    <taskList>        <task name="Select">            <discription>                This action is going to run 5 SELECT statements against both the empMDC and empNoMDC tables and records the execution times.             </discription>            <action  name="runMultiSelect" type="blank">                <followOnAction condition="true" compareOn="true" compareOnType="raw">                    <task>                        <echo>Running statements:</echo>                        <echo>                    1)  FROM ?SCHEMA?.empMDC WHERE dept = 5 AND div = 2  GROUP BY div, dept</echo>                        <echo>                    2)  FROM ?SCHEMA?.empNoMDC WHERE dept = 5 AND div = 2  GROUP BY div, dept</echo>                        <action  name="runSelect" type="serverAction">                            <parameterList>                                <parameter name="action" type="fixed">                                    <value>run_workload</value>                                </parameter>                                <parameter name="SQL[1]" type="fixed">                                    <value>FROM ?SCHEMA?.empMDC WHERE dept = 5 AND div = 2  GROUP BY div, dept</value>                                </parameter>                                <parameter name="SQL[2]" type="fixed">                                    <value>FROM ?SCHEMA?.empNoMDC WHERE dept = 5 AND div = 2  GROUP BY div, dept</value>                                </parameter>                                <parameter name="Repetition" type="fixed">                                    <value>5</value>                                </parameter>                                <parameter name="SampleRate" type="fixed">                                    <value>1</value>                                </parameter>                            </parameterList>                            <followOnAction condition="true">                                <task>                                    <assignSharedConstant name="SELECT5TIME_MDC" type="returnObject">                                        <value>runSelect.returnValue.SQL.1</value>                                    </assignSharedConstant>                                    <echo>Running time [regular table]: ?SELECT5TIME_REG?s</echo>                                </task>                            </followOnAction>                            <followOnAction condition="true">                                <task>                                    <assignSharedConstant name="SELECT5TIME_REG" type="returnObject">                                        <value>runSelect.returnValue.SQL.2</value>                                    </assignSharedConstant>                                    <echo>Running time [MDC table]: ?SELECT5TIME_MDC?s</echo>                                </task>                            </followOnAction>                            <followOnAction condition="false">                                <task>                                    <echo>Couldn't run the query, is your connection active?</echo>                                </task>                            </followOnAction>                        </action>                    </task>                </followOnAction>            </action>            </task>                                                                            <task name="Visualize">            <discription>                We are now going to graph the data we just acquired.            </discription>            <action  name="runSelect" type="blank">                <followOnAction condition="true" compareOn="true" compareOnType="raw">                    <task>                        <echo>Graphing times</echo>                        <loadPage>                            <pageWindow target="informationWindow">                                <panel name="main">                                    <link type="action" target="_self" window="_self" connectionRequired="y">                                        <parameterList>                                            <parameter name="action">graphYUI</parameter>                                            <parameter name="graph">	                                                <![CDATA[					       				                                                  {	                                                "Graph": 	                                                {		                                                "title": "MDC vs. Regular Table Select Performance",		                                                "graphType": "column",                                                        "xField": "attempt",		                                                "forceYBaseline": "true",		                                                "forceXBaseline": "true",		                                                "style":{	"legend": { "display": "right" }, "animationEnabled" : "false"},		                                                "responseSchema": {"fields": ["attempt", "with_mdc", "without_mdc"]},		                                                "seriesDef": [                                                                        {yField:"with_mdc", displayName:"Using an MDC Table"},                                                                        {yField:"without_mdc", displayName:"Using a Regular Table"},                                                                     ],		                                                "datasets":			                                                 [                                                                     				                                                    {"attempt": "Attempt #1", "with_mdc": [?SELECT5TIME_REG?][0], "without_mdc": [?SELECT5TIME_MDC?][0]},				                                                    {"attempt": "Attempt #2", "with_mdc": [?SELECT5TIME_REG?][1], "without_mdc": [?SELECT5TIME_MDC?][1]},				                                                    {"attempt": "Attempt #3", "with_mdc": [?SELECT5TIME_REG?][2], "without_mdc": [?SELECT5TIME_MDC?][2]},				                                                    {"attempt": "Attempt #4", "with_mdc": [?SELECT5TIME_REG?][3], "without_mdc": [?SELECT5TIME_MDC?][3]},				                                                    {"attempt": "Attempt #5", "with_mdc": [?SELECT5TIME_REG?][4], "without_mdc": [?SELECT5TIME_MDC?][4]}						                                                ]	                                                }                                                }                                                ]]>                                                                          </parameter>                                        </parameterList>                                    </link>				                                </panel>                            </pageWindow>                        </loadPage>                    </task>	                </followOnAction>            </action>        </task>     </taskList></actionPanel>