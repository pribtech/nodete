<table name="pgaStat">
	<sqlName>V$PGASTAT</sqlName>
	<singularName>PGA Statistics</singularName>
	<pluralName>PGA Statistics</pluralName>
	<description></description>
	<output>
		<type name="DDL" destination="" title="" generator=""/>
	</output>
   	<disable_edit/>
	<orderBy name="NAME" direction="A"/>
	<displayColumns>
		<col type="column" name="NAME"/>
		<col type="column" name="VALUE"/>
		<col type="column" name="UNIT"/>
	</displayColumns>
	<column name="NAME">
		<sqlName>NAME</sqlName>
		<title>Name</title>
		<type>s</type>
		<drillEnable/>
	</column>
	<column name="VALUE">
		<sqlName>VALUE</sqlName>
		<title>Value</title>
		<type>n</type>
		<formatNumber toAbbreviated="4"/> 
	</column>
	<column name="UNIT">
		<sqlName>UNIT</sqlName>
		<title>Unit</title>
		<type>s</type>
		<drillEnable/>
	</column>
</table>
