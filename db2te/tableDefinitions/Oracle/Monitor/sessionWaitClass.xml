<table name="sessionWaitClass">
	<sqlName>V$SESSION_WAIT_CLASS</sqlName>
	<singularName>Session Wait Class</singularName>
	<pluralName>Session Wait Classes</pluralName>
	<description></description>
	<output>
		<type name="DDL" destination="" title="" generator=""/>
	</output>	
	<orderBy name="SID" direction="A"/> 
	<orderBy name="SERIAL#" direction="A"/> 
	<displayColumns>
		<col type="column" name="SID"/>
		<col type="column" name="SERIAL#"/>
		<col type="column" name="WAIT_CLASS_ID"/>
		<col type="column" name="WAIT_CLASS#"/>
		<col type="column" name="WAIT_CLASS"/>
		<col type="column" name="TOTAL_WAITS"/>
		<col type="column" name="TIME_WAITED"/>
	</displayColumns>
	<column name="SID">
		<sqlName>SID</sqlName>
		<title>SID</title>
		<type>n</type>
	</column>
	<column name="SERIAL#">
		<sqlName>SERIAL#</sqlName>
		<title>Serial No.</title>
		<type>n</type>
	</column>
	<column name="WAIT_CLASS_ID">
		<sqlName>WAIT_CLASS_ID</sqlName>
		<title>Wait Class Id</title>
		<type>n</type>
	</column>
	<column name="WAIT_CLASS#">
		<sqlName>WAIT_CLASS#</sqlName>
		<title>Wait Class No.</title>
		<type>n</type>
	</column>
	<column name="WAIT_CLASS">
		<sqlName>WAIT_CLASS</sqlName>
		<title>Wait Class</title>
		<type>s</type>
		<drillEnable/>
	</column>
	<column name="TOTAL_WAITS">
		<sqlName>TOTAL_WAITS</sqlName>
		<title>Total Waits</title>
		<type>n</type>
	</column>
	<column name="TIME_WAITED">
		<sqlName>TIME_WAITED</sqlName>
		<title>Time Waited</title>
		<type>n</type>
	</column>
</table>
