		<type>n</type>
	</column>
	<column name="NUM_MSGS">
		<sqlName>NUM_MSGS</sqlName>
		<title>Num Msgs</title>
		<type>n</type>
	</column>
	<column name="CNUM_MSGS">
		<sqlName>CNUM_MSGS</sqlName>
		<title>Cnum Msgs</title>
		<type>n</type>
	</column>
	<column name="TOTAL_DEQUEUED_MSG">
		<sqlName>TOTAL_DEQUEUED_MSG</sqlName>
		<title>Total Dequeued Msg</title>
		<type>n</type>
	</column>
	<column name="TOTAL_SPILLED_MSG">
		<sqlName>TOTAL_SPILLED_MSG</sqlName>
		<title>Total Spilled Msg</title>
		<type>n</type>
	</column>
