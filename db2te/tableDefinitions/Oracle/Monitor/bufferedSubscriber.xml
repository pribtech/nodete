		<type>s</type>
		<drillEnable/>
	</column>
	<column name="SUBSCRIBER_ID">
		<sqlName>SUBSCRIBER_ID</sqlName>
		<title>Subscriber Id</title>
		<type>n</type>
	</column>
	<column name="SUBSCRIBER_NAME">
		<sqlName>SUBSCRIBER_NAME</sqlName>
		<title>Subscriber</title>
		<type>s</type>
		<drillEnable/>
	</column>
	<column name="SUBSCRIBER_ADDRESS">
		<sqlName>SUBSCRIBER_ADDRESS</sqlName>
		<title>Subscriber Address</title>
		<type>s</type>
		<drillEnable/>
	</column>
	<column name="PROTOCOL">
		<sqlName>PROTOCOL</sqlName>
		<title>Protocol</title>
		<type>n</type>
	</column>
	<column name="SUBSCRIBER_TYPE">
		<sqlName>SUBSCRIBER_TYPE</sqlName>
		<title>Subscriber Type</title>
		<type>s</type>
		<drillEnable/>
	</column>
	<column name="STARTUP_TIME">
		<sqlName>STARTUP_TIME</sqlName>
		<title>Startup Time</title>
		<type>n</type>
		<drillEnable/>
	</column>
	<column name="LAST_BROWSED_SEQ">
		<sqlName>LAST_BROWSED_SEQ</sqlName>
		<title>Last Browsed Seq</title>
		<type>n</type>
	</column>
	<column name="LAST_BROWSED_NUM">
		<sqlName>LAST_BROWSED_NUM</sqlName>
		<title>Last Browsed Num</title>
		<type>n</type>
	</column>
	<column name="LAST_DEQUEUED_SEQ">
		<sqlName>LAST_DEQUEUED_SEQ</sqlName>
		<title>Last Dequeued Seq</title>
		<type>n</type>
	</column>
