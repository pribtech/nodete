<table name="bufferedSubscriber">
	<sqlName>V$BUFFERED_SUBSCRIBERS</sqlName>
	<singularName>Buffered Subscriber</singularName>
	<pluralName>Buffered Subscribers</pluralName>
	<description></description>
	<orderBy name="QUEUE_SCHEMA" direction="A"/>
	<orderBy name="QUEUE_NAME" direction="A"/>
	<orderBy name="SUBSCRIBER_NAME" direction="A"/>
	<displayColumns>
		<col type="column" name="QUEUE_SCHEMA"/>
		<col type="column" name="QUEUE_NAME"/>
		<col type="column" name="SUBSCRIBER_NAME"/>
		<col type="column" name="PROTOCOL"/>
		<col type="column" name="SUBSCRIBER_TYPE"/>
		<col type="column" name="STARTUP_TIME"/>
		<col type="column" name="LAST_BROWSED_SEQ"/>
		<col type="column" name="LAST_BROWSED_NUM"/>
		<col type="column" name="LAST_DEQUEUED_SEQ"/>
		<col type="column" name="LAST_DEQUEUED_NUM"/>
		<col type="column" name="CURRENT_ENQ_SEQ"/>
		<col type="column" name="NUM_MSGS"/>
		<col type="column" name="CNUM_MSGS"/>
		<col type="column" name="TOTAL_DEQUEUED_MSG"/>
		<col type="column" name="TOTAL_SPILLED_MSG"/>
		<col type="column" name="EXPIRED_MSGS"/>
		<col type="column" name="MESSAGE_LAG"/>
	</displayColumns>
	<column name="QUEUE_ID">
		<sqlName>QUEUE_ID</sqlName>
		<title>QID</title>
		<type>n</type>
	</column>
	<column name="QUEUE_SCHEMA">
		<sqlName>QUEUE_SCHEMA</sqlName>
		<title>Schema</title>
		<type>s</type>
		<drillEnable/>
	</column>
	<column name="QUEUE_NAME">
		<sqlName>QUEUE_NAME</sqlName>
