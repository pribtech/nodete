		<title>Policy</title>
		<type>s</type>
		<drillEnable/>
	</column>
	<column name="ESTIMATED_OPTIMAL_SIZE">
		<sqlName>ESTIMATED_OPTIMAL_SIZE</sqlName>
		<title>Estimated Optimal Size</title>
		<type>n</type>
		<formatNumber toAbbreviated="4"/> 
	</column>
	<column name="ESTIMATED_ONEPASS_SIZE">
		<sqlName>ESTIMATED_ONEPASS_SIZE</sqlName>
		<title>Estimated Onepass Size</title>
		<type>n</type>
		<formatNumber toAbbreviated="4"/> 
	</column>
