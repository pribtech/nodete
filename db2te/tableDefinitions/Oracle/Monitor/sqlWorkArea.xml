		<type>n</type>
		<formatNumber toAbbreviated="4"/> 
	</column>
	<column name="LAST_TEMPSEG_SIZE">
		<sqlName>LAST_TEMPSEG_SIZE</sqlName>
		<title>Last Temp Seg. Size</title>
		<type>n</type>
		<formatNumber toAbbreviated="4"/> 
	</column>
</table>
