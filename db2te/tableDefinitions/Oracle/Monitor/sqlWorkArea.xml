		<sqlName>LAST_EXECUTION</sqlName>
		<title>Last Execution</title>
		<type>s</type>
		<drillEnable/>
	</column>
	<column name="LAST_DEGREE">
		<sqlName>LAST_DEGREE</sqlName>
