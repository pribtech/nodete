		<type>n</type>
	</column>
	<column name="HASH_VALUE">
		<sqlName>HASH_VALUE</sqlName>
		<title>Hash Value</title>
		<type>n</type>
	</column>
	<column name="SQL_ID">
		<sqlName>SQL_ID</sqlName>
		<title>SQL Id.</title>
		<type>s</type>
		<drillEnable/>
	</column>
	<column name="CHILD_NUMBER">
		<sqlName>CHILD_NUMBER</sqlName>
		<title>Child Number</title>
		<type>n</type>
	</column>
	<column name="WORKAREA_ADDRESS">
		<sqlName>WORKAREA_ADDRESS</sqlName>
		<title>Workarea Address</title>
		<type>n</type>
	</column>
	<column name="OPERATION_TYPE">
		<sqlName>OPERATION_TYPE</sqlName>
		<title>Operation Type</title>
		<type>s</type>
		<drillEnable/>
	</column>
	<column name="OPERATION_ID">
		<sqlName>OPERATION_ID</sqlName>
		<title>Operation Id</title>
		<type>n</type>
	</column>
	<column name="POLICY">
