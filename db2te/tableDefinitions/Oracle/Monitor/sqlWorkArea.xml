		<type>n</type>
		<formatNumber toAbbreviated="4"/> 
	</column>
	<column name="OPTIMAL_EXECUTIONS">
		<sqlName>OPTIMAL_EXECUTIONS</sqlName>
		<title>Optimal Executions</title>
		<type>n</type>
		<formatNumber toAbbreviated="4"/> 
	</column>
	<column name="ONEPASS_EXECUTIONS">
		<sqlName>ONEPASS_EXECUTIONS</sqlName>
		<title>Onepass Executions</title>
		<type>n</type>
		<formatNumber toAbbreviated="4"/> 
	</column>
