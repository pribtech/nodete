<table name="sqlWorkArea">
	<sqlName>V$SQL_WORKAREA</sqlName>
	<singularName>SQL Work Area</singularName>
	<pluralName>SQL Work Areas</pluralName>
	<description></description>
	<output>
		<type name="DDL" destination="" title="" generator=""/>
	</output>
   	<disable_edit/>
	<orderBy name="SQL_ID" direction="A"/>
	<displayColumns>
		<col type="column" name="HASH_VALUE"/>
		<col type="column" name="SQL_ID"/>
		<col type="column" name="CHILD_NUMBER"/>
		<col type="column" name="WORKAREA_ADDRESS"/>
		<col type="column" name="OPERATION_TYPE"/>
		<col type="column" name="OPERATION_ID"/>
		<col type="column" name="POLICY"/>
		<col type="column" name="ESTIMATED_OPTIMAL_SIZE"/>
		<col type="column" name="ESTIMATED_ONEPASS_SIZE"/>
		<col type="column" name="LAST_MEMORY_USED"/>
		<col type="column" name="LAST_EXECUTION"/>
		<col type="column" name="LAST_DEGREE"/>
		<col type="column" name="TOTAL_EXECUTIONS"/>
		<col type="column" name="OPTIMAL_EXECUTIONS"/>
		<col type="column" name="ONEPASS_EXECUTIONS"/>
		<col type="column" name="MULTIPASSES_EXECUTIONS"/>
		<col type="column" name="ACTIVE_TIME"/>
		<col type="column" name="MAX_TEMPSEG_SIZE"/>
		<col type="column" name="LAST_TEMPSEG_SIZE"/>
