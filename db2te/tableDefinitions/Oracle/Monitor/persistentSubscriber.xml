		<sqlName>LAST_ENQUEUE_TIME</sqlName>
		<title>Last Enqueue Time</title>
		<type>n</type>
	</column>
	<column name="LAST_DEQUEUE_TIME">
		<sqlName>LAST_DEQUEUE_TIME</sqlName>
		<title>Last Dequeue Time</title>
		<type>n</type>
	</column>
</table>