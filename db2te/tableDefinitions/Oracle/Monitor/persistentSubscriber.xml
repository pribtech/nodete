		<type>s</type>
		<drillEnable/>
	</column>
	<column name="SUBSCRIBER_ID">
		<sqlName>SUBSCRIBER_ID</sqlName>
		<title>Subscriber Id</title>
		<type>n</type>
	</column>
	<column name="SUBSCRIBER_NAME">
		<sqlName>SUBSCRIBER_NAME</sqlName>
		<title>Subscriber Name</title>
		<type>s</type>
		<drillEnable/>
	</column>
	<column name="SUBSCRIBER_ADDRESS">
		<sqlName>SUBSCRIBER_ADDRESS</sqlName>
		<title>Subscriber Address</title>
		<type>s</type>
		<drillEnable/>
	</column>
	<column name="PROTOCOL">
		<sqlName>PROTOCOL</sqlName>
		<title>Protocol</title>
		<type>n</type>
