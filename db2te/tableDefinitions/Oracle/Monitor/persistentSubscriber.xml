<table name="persistentSubscriber">
	<sqlName>V$PERSISTENT_SUBSCRIBERS</sqlName>
	<singularName>Persistent Subscriber</singularName>
	<pluralName>Persistent Subscribers</pluralName>
	<description></description>
	<orderBy name="QUEUE_SCHEMA" direction="A"/>
	<orderBy name="QUEUE_NAME" direction="A"/>
	<orderBy name="SUBSCRIBER_NAME" direction="A"/>
	<displayColumns>
		<col type="column" name="QUEUE_SCHEMA"/>
		<col type="column" name="QUEUE_NAME"/>
		<col type="column" name="SUBSCRIBER_NAME"/>
		<col type="column" name="SUBSCRIBER_ADDRESS"/>
		<col type="column" name="PROTOCOL"/>
		<col type="column" name="SUBSCRIBER_TYPE"/>
		<col type="column" name="FIRST_ACTIVITY_TIME"/>
		<col type="column" name="ENQUEUED_MSGS"/>
		<col type="column" name="DEQUEUED_MSGS"/>
		<col type="column" name="BROWSED_MSGS"/>
		<col type="column" name="EXPIRED_MSGS"/>
		<col type="column" name="DEQUEUED_MSG_LATENCY"/>
		<col type="column" name="LAST_ENQUEUE_TIME"/>
		<col type="column" name="LAST_DEQUEUE_TIME"/>
	</displayColumns>
	<column name="QUEUE_ID">
		<sqlName>QUEUE_ID</sqlName>
		<title>QID</title>
		<type>n</type>
	</column>
	<column name="QUEUE_SCHEMA">
		<sqlName>QUEUE_SCHEMA</sqlName>
		<title>Schema</title>
		<type>s</type>
		<drillEnable/>
	</column>
	<column name="QUEUE_NAME">
		<sqlName>QUEUE_NAME</sqlName>
