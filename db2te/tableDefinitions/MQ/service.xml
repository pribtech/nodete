<table name="service">
	<sqlName>service</sqlName>
	<singularName>Service</singularName>
	<pluralName>Services</pluralName>
	<inlinequery>
<![CDATA[
MQCMD_INQUIRE_SERVICE  ?
]]>
	</inlinequery>
	<parameters>
		<parm name="MQCA_SERVICE_NAME">
			<title>Service Mask</title>
			<value>*</value>
			<type>s</type>
		</parm>
	</parameters>
	<displayColumns>
		<col type="column" name="MQCA_SERVICE_NAME"/>
		<col type="column" name="MQIA_SERVICE_CONTROL"/>
		<col type="column" name="MQIA_SERVICE_TYPE"/>
		<col type="column" name="MQCA_SERVICE_START_COMMAND"/>
		<col type="column" name="MQCA_SERVICE_START_ARGS"/>
		<col type="column" name="MQCA_SERVICE_STOP_COMMAND"/>
		<col type="column" name="MQCA_SERVICE_STOP_ARGS"/>
		<col type="column" name="MQCA_STDOUT_DESTINATION"/>
		<col type="column" name="MQCA_STDERR_DESTINATION"/>
		<col type="column" name="MQCA_SERVICE_DESC"/>
		<col type="column" name="MQCA_ALTERATION_DATE"/>
		<col type="column" name="MQCA_ALTERATION_TIME"/>
	</displayColumns>
	<column name="MQCA_SERVICE_NAME">
		<sqlName>MQCA_SERVICE_NAME</sqlName>
		<title>Name</title>
	</column>
	<column name="MQIA_SERVICE_CONTROL">
		<sqlName>MQIA_SERVICE_CONTROL</sqlName>
		<title>Control</title>
	</column>
	<column name="MQIA_SERVICE_TYPE">
		<sqlName>MQIA_SERVICE_TYPE</sqlName>
		<title>Type</title>
		<type>n</type>
	</column>
	<column name="MQCA_SERVICE_START_COMMAND">
		<sqlName>MQCA_SERVICE_START_COMMAND</sqlName>
		<title>Start Command</title>
	</column>
	<column name="MQCA_SERVICE_START_ARGS">
		<sqlName>MQCA_SERVICE_START_ARGS</sqlName>
		<title>Start Args</title>
	</column>
	<column name="MQCA_SERVICE_STOP_COMMAND">
		<sqlName>MQCA_SERVICE_STOP_COMMAND</sqlName>
		<title>Stop Command</title>
	</column>
	<column name="MQCA_SERVICE_STOP_ARGS">
		<sqlName>MQCA_SERVICE_STOP_ARGS</sqlName>
		<title>Stop Args</title>
	</column>
	<column name="MQCA_STDOUT_DESTINATION">
		<sqlName>MQCA_STDOUT_DESTINATION</sqlName>
		<title>Stdout Destination</title>
	</column>
	<column name="MQCA_STDERR_DESTINATION">
		<sqlName>MQCA_STDERR_DESTINATION</sqlName>
		<title>Stderr Destination</title>
	</column>
	<column name="MQCA_SERVICE_DESC">
		<sqlName>MQCA_SERVICE_DESC</sqlName>
		<title>Description</title>
	</column>
	<column name="MQCA_ALTERATION_DATE">
		<sqlName>MQCA_ALTERATION_DATE</sqlName>
		<title>Alteration Date</title>
	</column>
	<column name="MQCA_ALTERATION_TIME">
		<sqlName>MQCA_ALTERATION_TIME</sqlName>
		<title>Alteration Time</title>
	</column>
</table>