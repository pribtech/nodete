<table name="monWorkload">
	<sql_name>monTest</sql_name>
	<singular_name>Workload Metrics</singular_name>
	<plural_name>Workload Metrics</plural_name>
	<inlinequery>
<![CDATA[
SELECT SERVICE_SUPERCLASS_NAME, AGENT_WAIT_TIME, POOL_READ_TIME, POOL_WRITE_TIME, CLIENT_IDLE_WAIT_TIME, DIRECT_READ_TIME, DIRECT_WRITE_TIME, FCM_RECV_WAIT_TIME, FCM_SEND_WAIT_TIME, IPC_RECV_WAIT_TIME, IPC_SEND_WAIT_TIME, LOCK_WAIT_TIME, LOG_BUFFER_WAIT_TIME, LOG_DISK_WAIT_TIME, TCPIP_RECV_WAIT_TIME, TCPIP_SEND_WAIT_TIME, TOTAL_APP_RQST_TIME, TOTAL_RQST_TIME, WLM_QUEUE_TIME_TOTAL, TOTAL_WAIT_TIME, TOTAL_SECTION_SORT_TIME, TOTAL_SECTION_SORT_PROC_TIME
	FROM TABLE(MON_GET_SERVICE_SUBCLASS('','',-2)) AS t
]]>
	</inlinequery>
	<messages><SQL0551N>Security issue. Please install "Monitoring Switches/Security" in Tools -> Installed Components</SQL0551N></messages>
